<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.carolsum.jingle">

  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ui.activity.HomeActivity" />
    <activity android:name=".ui.activity.RegisterActivity" />
    <activity android:name=".ui.activity.RegisterSuccessActivity" />
    <activity android:name=".ui.activity.NotificationActivity" />
    <activity android:name=".ui.activity.SettingsActivity" />
    <activity android:name=".ui.activity.CertificationActivity" />
    <activity android:name=".ui.activity.ProfileSettingActivity" />
    <activity android:name=".ui.activity.SponsorListActivity" />
    <activity android:name=".ui.activity.AcceptListActivity"></activity>
  </application>

</manifest>
